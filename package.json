{
  "name": "reactssr",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "webpack:server": "webpack --mode=development --config webpack.server.js --watch",
    "webpack:client": "webpack --mode=development --config webpack.client.js --watch",
    "webpack:start": "nodemon --watch build --exec node build/server.bundle.js",
    "dev": "npm-run-all --parallel webpack:*",
    "build": "webpack --mode=production --config webpack.server.js && webpack --mode=production --config webpack.client.js",
    "deploy:ssr": "node build/server.bundle.js"
  },
  "dependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.3.1",
    "css-loader": "^2.1.1",
    "express": "^4.16.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^3.0.1",
    "global": "^4.3.2",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.0",
    "npm-run-all": "^4.1.5",
    "react": "^16.8.6",
    "react-bootstrap": "^1.0.0-beta.8",
    "react-dom": "^16.8.6",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.2",
    "webpack-node-externals": "^1.7.2"
  }
}
